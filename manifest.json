{
  "manifest_version"  : 2,
  "name"              : "Github Menlo",
  "version"           : "1.1",
  "description"       : "Resets monospace fonts on github.com sites to Menlo",
  "permissions"       : [
                          "tabs",
                          "http://*/*"
                        ],
  "content_scripts"   : [
    {
      "matches"       : [ "*://github.com/*", "*://*.github.com/*" ],
      "js"            : [ "index.js" ]
    }
  ]
}
